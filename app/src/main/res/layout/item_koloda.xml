        android:textSize="16sp"
        android:typeface="serif"
        app:layout_constraintStart_toEndOf="@+id/tvRActivitiesInput2"
        app:layout_constraintTop_toBottomOf="@+id/tvRInterestsInput3"
        tools:text="Activity3" />

    <TextView
        android:id="@+id/tvRActivitiesInput2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:padding="5dp"
        android:paddingStart="8dp"
        android:paddingLeft="5dp"
        android:paddingTop="5dp"
        android:paddingEnd="8dp"
        android:paddingRight="5dp"
        android:paddingBottom="5dp"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:typeface="serif"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvRInterestsInput2"
        tools:text="Activity2" />

    <TextView
        android:id="@+id/tvRInterestsInput3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="10dp"
        android:padding="5dp"
        android:paddingStart="8dp"
        android:paddingLeft="5dp"
        android:paddingTop="5dp"
        android:paddingEnd="8dp"
        android:paddingRight="5dp"
