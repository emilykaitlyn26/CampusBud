        android:id="@+id/tvRDrinkingInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="5dp"
        android:textSize="16sp"
        android:typeface="serif"
        app:layout_constraintStart_toEndOf="@+id/tvRTimeWake"
        app:layout_constraintTop_toBottomOf="@+id/tvRSmokingInput"
        tools:text="No" />

    <TextView
        android:id="@+id/tvRRoomUse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:text="Room Use:"
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="serif"
        app:layout_constraintStart_toEndOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/tvRDrinking" />

    <TextView
        android:id="@+id/tvRRoomUseInput"
        android:layout_width="wrap_content"
