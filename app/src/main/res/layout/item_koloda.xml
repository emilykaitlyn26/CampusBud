        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:typeface="serif"
        app:layout_constraintEnd_toStartOf="@+id/tvRActivitiesInput2"
        app:layout_constraintTop_toBottomOf="@+id/tvRInterestsInput1"
        tools:text="Activity1" />

