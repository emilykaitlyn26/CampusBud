<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.ProfileFragment">

    <TextView
        android:id="@+id/tvTimeSleepInput2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="3dp"
        android:text="8:00am"
        app:layout_constraintStart_toEndOf="@+id/tvTimeWake"
        app:layout_constraintTop_toBottomOf="@+id/tvTimeSleepInput" />

    <TextView
        android:id="@+id/tvTimeWake"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="3dp"
        android:text="Time Wake Up: "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvTimeSleep" />

    <TextView
        android:id="@+id/tvTimeSleepInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="3dp"
        android:text="10:00pm"
        app:layout_constraintStart_toEndOf="@+id/tvTimeSleep"
        app:layout_constraintTop_toBottomOf="@+id/tvRoomUseDisplay2" />

    <TextView
        android:id="@+id/tvRoomUseDisplay2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="3dp"
        android:text="Preferred Room Use: "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvDrinking" />

    <TextView
        android:id="@+id/tvTimeNight2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:text="When do you sleep?"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rgRoomUse" />

    <ImageView
        android:id="@+id/ivPictureDisplay"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="26dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/addprofile"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/tvUserDisplay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="34dp"
        app:layout_constraintStart_toEndOf="@+id/ivPictureDisplay"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="emilykaitlyn" />

    <TextView
        android:id="@+id/tvYearDisplay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toEndOf="@+id/ivPictureDisplay"
        app:layout_constraintTop_toBottomOf="@+id/tvUserDisplay"
        tools:text="Junior" />

    <TextView
        android:id="@+id/tvMajorDisplay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="11dp"
        app:layout_constraintStart_toEndOf="@+id/ivPictureDisplay"
        app:layout_constraintTop_toBottomOf="@+id/tvYearDisplay"
        tools:text="Computer Science" />

    <ImageView
        android:id="@+id/ivSettings"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/settingsicon"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/tvRoommateProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="14dp"
        android:text="Roommate Profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivPictureDisplay" />

    <TextView
        android:id="@+id/tvRoomCleanliness"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="8dp"
        android:text="Room Cleanliness: "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvRoommateProfile" />

    <TextView
        android:id="@+id/tvSmoking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="4dp"
        android:text="Smoking: "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvRoomCleanliness" />

    <TextView
        android:id="@+id/tvDrinking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="4dp"
        android:text="Drinking: "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvSmoking" />

    <TextView
        android:id="@+id/tvCleanlinessInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toEndOf="@+id/tvRoomCleanliness"
        app:layout_constraintTop_toBottomOf="@+id/tvRoommateProfile"
        tools:text="Organized" />

    <TextView
        android:id="@+id/tvSmokingInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@+id/tvSmoking"
        app:layout_constraintTop_toBottomOf="@+id/tvRoomCleanliness"
        tools:text="No" />

    <TextView
        android:id="@+id/tvDrinkingInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@+id/tvDrinking"
        app:layout_constraintTop_toBottomOf="@+id/tvSmoking"
        tools:text="No" />

    <TextView
        android:id="@+id/tvTimeSleep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="3dp"
        android:text="Time Sleep: "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvRoomUseDisplay2" />

    <TextView
        android:id="@+id/tvRoomUseInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="3dp"
        android:text="Social Space"
        app:layout_constraintStart_toEndOf="@+id/tvRoomUseDisplay2"
        app:layout_constraintTop_toBottomOf="@+id/tvDrinkingInput" />

</androidx.constraintlayout.widget.ConstraintLayout>